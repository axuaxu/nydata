      this.hideResults();

      // Take out artificial spacing
      query = query.split('&nbsp;').join('');
      query = query.split('<br>').join('');
      query = query.split('&amp;').join('&');

      // Cancel the previous request
      if(this.AJAXRequest) {
        this.AJAXRequest.abort();
      }

      // Trim extra whitespace
      query = $.trim(query)
      if(query.length > 1) {
        // Load the search results
