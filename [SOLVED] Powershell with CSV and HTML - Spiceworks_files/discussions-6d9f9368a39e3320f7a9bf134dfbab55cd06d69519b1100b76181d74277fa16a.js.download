      this.spicedState = !this.spicedState;
      this.model.set('vote_count', this.model.get('vote_count') + (this.spicedState ? 1 : -1) );
      this.spiceModel.set({ state: this.spicedState, count: this.model.get('vote_count') });
    }
  });

