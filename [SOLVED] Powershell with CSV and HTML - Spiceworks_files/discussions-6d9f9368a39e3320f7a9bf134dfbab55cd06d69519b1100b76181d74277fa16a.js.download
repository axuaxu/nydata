var JoinAndLogin = {
  modalShowing: (window.location.pathname == '/join' || window.location.pathname == '/login'),
  modePreference: {
    'mode=reset': function(){
      var modal = jQuery('a[data-toggle="password_reset"]').parents('.sui-modal_content');
